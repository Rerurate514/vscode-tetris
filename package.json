{
  "name": "vscode-tetris",
  "displayName": "vscode-tetris",
  "description": "",
  "version": "0.0.1",
  "engines": {
		"vscode": "^1.74.0"
	},
	"extensionKind": [
		"ui",
		"workspace"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"type": "webview",
					"id": "vscode-tetris.fieldView",
					"name": "vscode-tetris"
				}
			]
		},
		"commands": [
			{
				"command": "calicoColors.addColor",
				"category": "Calico Colors",
				"title": "Add Color"
			},
			{
				"command": "calicoColors.clearColors",
				"category": "Calico Colors",
				"title": "Clear Colors",
				"icon": "$(clear-all)"
			},
			{
				"command": "vscode-tetris.startGame",
				"category": "vscode-tetris",
				"title": "Game Start"
			},
			{
				"command": "vscode-tetris.finishGame",
				"category": "vscode-tetris",
				"title": "Game Finish"
			},
			{
				"command": "vscode-tetris.pauseGame",
				"category": "vscode-tetris",
				"title": "Game Pause"
			},
			{
				"command": "vscode-tetris.resetGame",
				"category": "vscode-tetris",
				"title": "Game Reset"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "calicoColors.clearColors",
					"group": "navigation",
					"when": "view == calicoColors.colorsView"
				},
				{
					"command": "vscode-tetris.resetGame",
					"group": "navigation",
					"when": "view == vscode-tetris.resetGame"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2",
		"eslint": "^8.32.0",
		"typescript": "^5.1.3"
	}
}